{
  "collectionName": "components_section_faq_items",
  "info": {
    "displayName": "FAQ Item",
    "description": "Single FAQ question and answer"
  },
  "options": {},
  "attributes": {
    "question": {
      "type": "string",
      "required": true,
      "maxLength": 200
    },
    "answer": {
      "type": "richtext",
      "required": true
    },
    "category": {
      "type": "enumeration",
      "enum": ["general", "products", "services", "shipping"],
      "default": "general"
    }
  }
}

